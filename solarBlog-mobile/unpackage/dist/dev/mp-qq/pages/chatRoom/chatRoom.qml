<view class="chatRoom"><view class="notice-box"><text class="notice">{{"当前有"+count+"人在线!"}}</text></view><view class="content-box"><scroll-view style="{{'height:'+(viewHeight+'px')+';'}}" scroll-y="{{true}}" scroll-into-view="{{viewId}}" data-event-opts="{{[['scrolltolower',[['scroll',['$event']]]]]}}" bindscrolltolower="__e"><block qq:for="{{$root.l0}}" qq:for-item="item" qq:for-index="__i0__" qq:key="id"><view class="userMessage-box" id="{{'item'+item.$orig.id}}"><view class="{{[item.$orig.ipAddress===ipAddress?'myHeader-box':'userHeader-box']}}"><image class="{{[item.$orig.ipAddress===ipAddress?'myAvatar':'userAvatar']}}" src="{{item.$orig.avatar}}"></image><view class="nickName-box"><text class="nickName">{{item.$orig.nickname}}</text><text class="date">{{item.g0}}</text></view></view><view class="{{[item.$orig.ipAddress===ipAddress?'myContent-box':'userContent-box']}}"><block qq:if="{{item.$orig.type===3}}"><view style="font-size:30rpx;"><rich-text nodes="{{item.$orig.content}}"></rich-text></view></block><block qq:if="{{item.$orig.type===5}}"><view><audio src="{{item.$orig.content}}"></audio></view></block></view><view class="both"></view></view></block></scroll-view></view><view data-ref="footer" class="footer vue-ref"><view class="content-wrap"><textarea class="content" maxlength="-1" data-event-opts="{{[['input',[['__set_model',['','message','$event',[]]]]]]}}" value="{{message}}" bindinput="__e"></textarea></view><view class="btn-wrap"><button class="{{['btn',(disable)?'disabled':'']}}" disabled="{{disable}}" data-event-opts="{{[['tap',[['handleSend']]]]}}" bindtap="__e">发送</button></view></view></view>